html {
	height: 100%;
	overflow: auto;
}
body {
	height: 100%;
	margin: 0;
	padding: 0;
    overflow-y: auto; 
	background-color: #3F1015;  
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:14px;
	color:#CCCCCC;   
}
#main {
	position: absolute;
	top: 0px; left: 0px;
	height: 100%;
	width:  100%;
}
a:link { 
    color:#FFFFCC;
}	
a:visited { 
    color: #CCCCCC;
}
a:hover { 
    color: #FFFFCC;
    text-decoration: none;
}
    
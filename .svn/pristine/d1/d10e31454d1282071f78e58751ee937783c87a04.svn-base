div.header {
	display: block;
	height: 120px;
}
.header_nav_container {
    margin: 0 auto;
    position: relative;
    width: 990px !important;
}
#inner-header {
    position: relative;
	display: block;
}
.onecol, .twocol, .threecol, .fourcol, .fivecol, .sixcol, .sevencol, .eightcol, .ninecol, .tencol, .elevencol, .twelvecol {
    float: left;
    position: relative;
}
.last {
    float: right;
}
.first {
    margin-left: 0;
}
.topheader-outer-fix {
	float: right;
    height: 35px;
    overflow: hidden;
}
.topheader-left-corner {
    background-image: url("../img/topheader-left-corner.png");
    background-repeat: no-repeat;
    float: left;
    height: 35px;
    min-width: 41px;
}
.topheader-menu {
    background: url("../img/topheader-menu-background.png") repeat-x scroll 0 0 #1C4A6C;
    float: left;
    height: 35px;
    padding: 5px 0 0;
    width: auto;
}
.topheader-right-corner {
    background-image: url("../img/topheader-right-corner.png");
    background-repeat: no-repeat;
    float: left;
    height: 35px;
    min-width: 41px;
}
.topheader-menu ul {
    margin: 0 !important;
    padding: 0 !important;
}
.topheader-menu a {
    color: #FFFFFF;
    cursor: pointer;
    display: inline-block;
    float: left;
    font-size: 14px;
    font-weight: bolder;
    padding: 0;
    text-decoration: none;
	font-family: Arial,Helvetica,sans-serif !important;
	line-height: 20px;
}
.topheader-menu a span {
    display: inline-block;
    float: right;
    padding: 2px 7px 5px 17px;
}
.topheader-menu a:visited {
	color:#FFF;
}
.topheader-menu a:hover {
	background:transparent url('../img/top-menu-right-corner.png') no-repeat scroll top right;
	outline: none;
	color:#fff !important;
}
.topheader-menu a:focus {
	color:#fff !important;
}
.topheader-menu a:hover span {
	background: url('../img/top-menu-left-corner.png') no-repeat;
	padding:2px 7px 5px 17px;
}
.topheader-menu a.selected {
	background: transparent url('../img/top-menu-right-corner.png') no-repeat scroll top right;
	color: #fff;
	outline: none;
}
.topheader-menu a.selected span {
	background: url('../img/top-menu-right-corner.png') no-repeat;
	padding:0px 12px 3px 28px;
}
.topheader-right-corner {
	min-width:41px;
	height:35px;
	background-image:url('../img/topheader-right-corner.png');
	background-repeat:no-repeat;
	float:left;
}
.topheader-menu ul li {
    color: #2D6C9A;
    float: left;
	line-height: 23px;
}
#addthis {
    padding: 15px;
}
nav {
    display: block;
}
.div-addthis {
    position: absolute;
    right: 16px;
    top: 58px;
	width: 250px;
}
#logo {
    float: left;
    font-size: 1.75em;
    height: 72px;
    margin: 10px 0 0 0 !important;
}
.h1 {
    font-size: 2.5em;
    line-height: 1.333em;
}
a, a:visited {
    color: #023961;
}
#menu-main {
    margin: 0 !important;
}
#menu-main {
    background: url("../img/main-menu-bg.png") repeat-x scroll 0 0 #3CC7F5;
}
#menu-main li {
    background: url("../img/main-menu-bg.png") repeat-x scroll 0 0 transparent;
    float: left;
    padding-left: 0;
    width: auto !important;
}
.nav li {
    float: left;
    position: relative;
}
.nav li {
}
.nav li a {
    background-color: transparent;
    border-bottom: 0 solid #CCCCCC;
    display: inline-block;
    float: left;
    padding: 10px;
    text-decoration: none;
}
.nav li a {
    border-bottom: 0 none;
}
#menu-main a {
    background: url("../img/main-menu-link-bg.png") repeat-y scroll right center transparent;
    color: #FFFFFF;
    display: inline-block;
    font-weight: 600;
    height: 38px;
    line-height: 38px;
    padding: 0 !important;
    text-align: center;
    width: 198px;
}
#menu-main .current_page_item a, #menu-main a:hover {
    background: url("../img/main-menu-hover.png") repeat scroll 0 0 transparent;
}


#nav {
	float: left;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
	width:975px;
	/*position:relative;*/
	z-index:1001;
	height: auto;
	background-color:#3ac1ee;
	
}
#nav li a, #nav li {
	float: left;
	font-size:18px;
	font-weight:normal;
	font-family: Arial, Helvetica, sans-serif;
	text-decoration:none;
	padding:0px 0px 0px 0px;
	line-height:39px;
	color:#fff;
}
#nav li {
	list-style: none;
	position: relative;
	width: auto;
	margin:0px 0px 0px 0px;
}
#nav li a {
	 font-size:18px; 
	 color:#fff; 
	 text-decoration:none; 
	 line-height:39px; 
	 padding:0px 53px 0px 53px; 
	 display:block; 
	 font-weight:normal;
	 font-family: Arial, Helvetica, sans-serif;
}
#nav li a:hover {
	color:#2b516f;
}

#nav li a.ctive{
	font-size:18px; 
	color:#fff; 
	text-decoration:none; 
	line-height:39px; 
	padding:0px 64px 0px 64px; 
	display:block;
	background-color:#2b516f;
}
	
	
/* Submenu */

.hasChildren {
	position: absolute;
	width: 0px;
	height: 0px;
	background: black;
	right : 0;
	bottom: 0;
	left:10px;
	
}
#nav li ul {
	width:1200px;	
	display: none;
	position: absolute;
	left:-327px;
	top:100%;
	padding: 0;
	margin: 0px 0px 0px 0px;
	background: url(../img/menu_bg.png) repeat;	
}
#nav li:hover > ul {
	display: block;
}
#nav li ul li {
	float:left;
	font-size:18px;
	display:block;
	color:#fff;
	padding:0px 0px 0px 0px;
	margin:0px 60px 0px 50px;
}
#nav li ul li a {
	float: none;
	display:block;
	font-size:18px;
	color:#fff;
	padding:0px 10px 0px 10px;
}
#nav li ul li, #nav li ul li a:hover {

}
#nav li ul li {
	_display: inline; /* for IE6 */
}
#nav li ul li a {
	width:auto;
	display: block;
	color:#fff;
	font-size:20px;
}
#nav li ul li a:hover {
	color:#ffbc12;
}
#nav li ul li a .location {
	font-size:24px;
}
/* SUBSUB Menu */

#nav li ul li ul {
	display: none;
}
#nav li ul li:hover ul {
	left: 100%;
	top: 0;
}


.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
 
.clearfix {
	display: inline-block;
}
 
html[xmlns] .clearfix {
	display: block;
}
 
* html .clearfix {
	height: 1%;
}
html {
	 height: 100%;
	 font-family: Verdana, Geneva, sans-serif;
	 font-size: 14px
}
body {
	position: relative;
	height: 100%;
	margin: 10px;
}
.me_left {
	float: left;
}
.me_right {
	float: right;
}
.me_absolute {
	position: absolute;
}
.me_relative {
	position: relative;
}
.me_hide {
	display: none;
}
ul {
	padding: 0;
	list-style: none;
}
p {
	margin: 0;
	padding: 0;
}
#main_container {
	position: absolute;
	width: 985px;
	left: 50%;
	margin-left: -492px;
	background: #feffff; 
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #feffff 10%, #95c2e4 100%); 
	background: -webkit-gradient(linear, left top, left bottom, color-stop(10%,#feffff), color-stop(100%,#95c2e4)); 
	background: -webkit-linear-gradient(top,  #feffff 10%,#95c2e4 100%); 
	background: -o-linear-gradient(top,  #feffff 10%,#95c2e4 100%); 
	background: -ms-linear-gradient(top,  #feffff 10%,#95c2e4 100%); 
	background: linear-gradient(to bottom,  #feffff 10%,#95c2e4 100%); 
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff', endColorstr='#95c2e4',GradientType=0 ); 
	padding: 0 60px 0 60px;
	border-top: 5px solid #24557a;
}
#explorer {
	padding: 15px;
	background-color: transparent;
}
#divider {
	width: 0;
	height: 100%;
	background-color: #fff;
}
#directory_dd_container {
	margin: 10px 15px;
}
#directory_dd {
	width:100%;
	height: 40px;
	background: transparent url(../images/dd-bg.png) repeat-x 0 0;
	border: 1px solid #d0d3d6;
	border-radius: 2px;
}
#directory_dd_options {
	border: solid 1px #CCC;
	border-top: none;
	list-style: none;
	box-shadow: 0px 1px 5px #DDD;
	position: absolute;
	z-index: 2000;
	margin: 0;
	padding: 0;
	background: white;
	overflow: auto;
	width: 280px;
	display: none;
}
.dd_option {
	padding: 10px;
	display: block;
	border-bottom: solid 1px #DDD;
	overflow: hidden;
	text-decoration: none;
	color: #333;
	cursor: pointer;
	-webkit-transition: all 0.25s ease-in-out;
	-moz-transition: all 0.25s ease-in-out;
	-o-transition: all 0.25s ease-in-out;
	-ms-transition: all 0.25s ease-in-out;
}
.dd_option:hover {
	background: #F3F3F3;
	color: black;
}
.dd_select:hover, .dd_option:hover {
	text-decoration: none;
}
.dd_select {
	overflow: hidden;
	display: block;
	padding: 10px;
	color: #5c595a;
	font-size: 16px;
	cursor: pointer;
}
.dd_pointer {
	width: 0;
	height: 0;
	right: 10px;
	top: 50%;
	margin-top: -3px;
}
.dd_pointer_down {
	border: solid 5px transparent;
	border-top: solid 5px black;
}
.dd_pointer_up{
	border: solid 5px transparent !important;
	border-bottom: solid 5px black !important;
	margin-top: -8px;
}

h2 {
   font-size: 1.5em;
   font-weight: bold;
   margin-bottom: .83em;
}

.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
.clearfix {
	display: inline-block;
	 zoom:1; /* For IE 6/7 (trigger hasLayout) */
}
 
html[xmlns] .clearfix {
	display: block;
}
 
* html .clearfix {
	height: 1%;
}
html, body  {
	outline:0px;
	padding:0px;
	border:0px;	
	margin:0px;
	font-family: Verdana, Geneva, sans-serif;
	font-size: 12px
}
body {
	background-image:url(../img/back-gradiant2.png);
	background-repeat:repeat-x;
	background-color:#94c1e3;
	outline:0px;
	padding:0px;
	border:0px;	
	margin:0px;
}
ul {
	margin: 0;
	padding: 0;
	list-style: none
}
.me_left {
	float: left;
}
.me_right {
	float: right;
}
.me_absolute {
	position: absolute;
}
.me_relative {
	position: relative;
}
.me_hide {
	display: none;
}
.me_clear {
	clear: both;
}
.boldify {
	font-weight: bold;
}
p {
	margin: 0;
	padding: 0;
}
#flashMessage {
	clear: both;
	background: green;
	color:#fff;
	font-size: 140%;
	font-weight: bold;
	padding: 5px;
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3);
	padding: 7px 14px;
	text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
	-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);
	-moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);
	box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);
}

#flashMessage.flash_error{
	background: #fff5f5;
	color: #dd4829;
	border: 1px solid red;
	border-radius: 4px;
	padding: 15px;
}

#flashMessage.flash_error i[class^="icon-"]{
	display: block;
	float: left;
	font-size: 64px;
	margin-right: 20px;
}

form div.error-message{
	color: red;
	font-size: 11px;
}

form label span.optional{
	color: #AAA;
	font-size: 11px;
}


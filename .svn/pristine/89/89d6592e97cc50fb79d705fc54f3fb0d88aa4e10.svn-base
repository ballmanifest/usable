	background: linear-gradient(to bottom,  rgba(254,254,254,1) 0%,rgba(249,249,249,1) 28%,rgba(246,246,246,1) 56%,rgba(245,245,245,1) 84%,rgba(250,250,250,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#fafafa',GradientType=0 );

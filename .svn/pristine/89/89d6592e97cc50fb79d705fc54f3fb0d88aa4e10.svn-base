/**
*	Fancy Alert Box
*/
#fancyAlertBox {
	line-height: 1.5em;
	text-align: center;
}
#fancyAlertBox .alert_message {
	text-align: center;
	font-size: 12px;
	color: #000;
	height: 70px;
	padding: 5px;
}
#fancyAlertBox .alert_box_buttons {
	width: 100%;
	text-align: center;
}
#fancyAlertBox .alert_box_buttons .alert_box_ok {
    background-color: #5BB75B;
    background-image: linear-gradient(to bottom, #62C462, #51A351);
    background-repeat: repeat-x;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    color: #FFFFFF;
    font-size: 12px;
    margin-top: 10px;
    padding: 2px 15px;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
	float: none;
	width: 40px;
}
#fancyAlertBox h5 {
	background: #376599;
	margin: 0;
	height: 25px;
	line-height: 25px;
	color: #fff;
	padding-left: 10px;
	margin-bottom: 10px;
	text-align: left;
	font-size: 13px;
}
#fancyAlertBox h5 span {
    background-color: #1F395A;
    border-radius: 16px 16px 16px 16px;
    float: right;
    font-size: 8px;
    height: 16px;
    line-height: 15px;
    margin: 3px;
    text-align: center;
    width: 16px;
	cursor: pointer;
}

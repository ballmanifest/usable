
.fc-dialog{
	background: url("../img/filocity-modal-bg.png") repeat-x scroll 0 0 #E6EFF6 !important;
	padding:0 !important;
	margin:0 !important;
	overflow:visible !important;
	border:1px solid #5DA1D1 !important;
	position:absolute !important;
	overflow:visible !important;
	float:none !important;
	border:1px solid #5DA1D1 !important;
	border-radius:0 !important;
	-webkit-border-radius:0 !important;
	-moz-border-radius:0 !important;
}
.fc-dialog-fixed-position{
	position:fixed !important;
}
.fc-dialog .ui-dialog-content{
	padding:0 !important;
	margin:0 !important;
	position:relative !important;
	overflow:hidden !important;
}
.fc-dialog .ui-dialog-titlebar{
	position:relative !important;
	overflow:visible !important;
	background:#5DA1D1 !important;
	padding:5px !important;
	font-weight:bold !important;
	font-size:14px !important;
	font-family:arial !important;
	color:#ffffff !important;
	cursor:move !important;
	overflow:visible !important;
	border:0 !important;
	border-radius:0 !important;
	-webkit-border-radius:0 !important;
	-moz-border-radius:0 !important;
	height:20px !important;
	line-height:20px !important;
	
}
.fc-dialog .ui-dialog-titlebar span{
	float:left;
	margin:0;
	padding:0;
}
.fc-dialog a.ui-dialog-titlebar-close {
	margin:0 !important;
	padding:0 !important;
	top:-20px !important;
	right:-20px !important;
	width:40px !important;
	height:40px !important;
	background:red !important;
	overflow:hidden !important;
	position:absolute !important;
	text-align:center !important;
	color:#ffffff !important;
	border:0 !important;
	border-radius:0 !important;
	-webkit-border-radius:0 !important;
	-moz-border-radius:0 !important;
	background:transparent url('../img/modal-close-button.png') no-repeat 0 0 !important;
}
.fc-dialog .ui-dialog-titlebar-close *{
	display:none !important;
}
.fc-dialog a.ui-dialog-titlebar-close:hover{
	background:transparent url('../img/modal-close-button.png') no-repeat -40px 0 !important;
}

.ui-widget-overlay{
	position:fixed !important;
	top:0 !important;
	left:0 !important;
	width:100% !important;
	height:100% !important;
	background:#ffffff !important;
}

.fc-dialog .filocity-modal-button{
    background: url("../img/filocity-modal-button.png") repeat-x scroll 0 0 transparent !important;
    border: 0 none !important;
    color: #FFFFFF !important;
    cursor: pointer !important;
    float: left !important;
    font-weight: bold !important;
    height: 28px !important;
    margin-top: 10px !important;
    padding: 0 5px !important;
	width:auto !important;
}
.fc-dialog .filocity-modal-button.pink{
	background: url("../img/filocity-modal-button.png") repeat-x scroll 0 -28px transparent !important;
}
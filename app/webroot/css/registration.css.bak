#wrapper_to_container_elements {	min-height : 705px !important;}#flashMessage.message {	display: none}#wrapper_to_container_elements {	padding-top: 0 !important;}#registration_form_container {	margin: 0 20px;	width: 700px}#link_header a.highlight, #link_header ul {    position: absolute;    top: 13px;}a.highlight {    color: #225478;    font-family: Arial,Verdana;    font-size: 14px;    font-weight: bold;    text-decoration: none;}.registration_form_wrapper {	background: transparent url(../img/features-text-bg.png) repeat-y 100% 0;}.reg_form_header {	height : 100px;	padding-top: 20px}.reg_form_header h1 {	font-size: 20px;	font-weight: bold;	word-spacing: -2px;	margin: 3px 0;}.reg_form_heading {	color: #204A68;}.reg_form_title {		color: #6AA9D6;}.reg_form_note, .note_to_login_info {	font-size : 12px;	color: gray}.reg_form_note strong, .form_section label, .caption a {	color: #5A5A5A;}.reg_form_sections .form_section {	float: left;	width:350px;}.login_info, .personal_info_section {	height: 190px;}.form_section div.input {	height: 40px;}.note_to_login_info {	margin-bottom: 28px}.form_section label {	font-size: 12px;	width: 125px;	display: inline-block;	font-weight: bold}.form_section input[type="text"], .form_section input[type="password"], .form_section select {	border: 1px solid #CCC;	height: 20px;	width: 188px;	position: absolute}.form_section select {	height : 23px}#UserSecurityCode {	width: 70px;	display: inline}#UserCardExpirationDateMonth, #UserCardExpirationDateYear {	width: 90px;	display: inline}div.password label {	line-height: 12px;}.security_code_holder {	width: 310px;	height: 40px}.security_code_img {	float: right}.section_header {	color: #333;	margin-bottom: 25px}.has_personal_address {	width: 250px;}.is_corporate_account {	width: 275px;}.is_agreed {	width: 295px;}.form_section .input.checkbox {	height: 40px;	line-height: 16px;}.caption {	margin: 0;	float: right;	font-size: 11px;	font-style: italic;	color: gray}div.submit input[type="submit"] {	background: transparent url(../img/reg_submit.png) no-repeat 0 0;	color: #fff;	border : none;	width: 105px;	height: 28px;	font-family: Arial;	font-size: 14px;	text-transform: uppercase;	margin: 20px 65px 20px 10px;	float: right;}div.error-message {	color: #f00;	font-size: 10px;}
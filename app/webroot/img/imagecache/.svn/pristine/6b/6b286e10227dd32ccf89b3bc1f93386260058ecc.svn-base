.data_progress {
	background: #fcfcfc;
	background: -moz-linear-gradient(left,  #fcfcfc 0%, #f3f3f3 100%, #ffffff 100%); 
	background: -webkit-gradient(linear, left top, right top, color-stop(0%,#fcfcfc), color-stop(100%,#f3f3f3), color-stop(100%,#ffffff)); 
	background: -webkit-linear-gradient(left,  #fcfcfc 0%,#f3f3f3 100%,#ffffff 100%); 
	background: -o-linear-gradient(left,  #fcfcfc 0%,#f3f3f3 100%,#ffffff 100%); 
	background: -ms-linear-gradient(left,  #fcfcfc 0%,#f3f3f3 100%,#ffffff 100%); 
	background: linear-gradient(to right,  #fcfcfc 0%,#f3f3f3 100%,#ffffff 100%); 
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fcfcfc', endColorstr='#ffffff',GradientType=1 ); 
    display: inline-block;
    height: 10px;
    margin: 0;
    overflow: hidden;
    position: relative;
    width: 122px;
	border-radius: 12px
}
.progress_percentile {
	width: 122px;
	border-radius: 12px;
	height: 10px;
	display: block;
	position: absolute;
	top: 0;
	left: -122px;
	background: #92d88f; 
	background: -moz-linear-gradient(left,  #92d88f 1%, #83c783 28%, #83c783 45%, #00b232 100%); 
	background: -webkit-gradient(linear, left top, right top, color-stop(1%,#92d88f), color-stop(28%,#83c783), color-stop(45%,#83c783), color-stop(100%,#00b232)); 
	background: -webkit-linear-gradient(left,  #92d88f 1%,#83c783 28%,#83c783 45%,#00b232 100%); 
	background: -o-linear-gradient(left,  #92d88f 1%,#83c783 28%,#83c783 45%,#00b232 100%); 
	background: -ms-linear-gradient(left,  #92d88f 1%,#83c783 28%,#83c783 45%,#00b232 100%); 
	background: linear-gradient(to right,  #92d88f 1%,#83c783 28%,#83c783 45%,#00b232 100%); 
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#92d88f', endColorstr='#00b232',GradientType=1 );
}


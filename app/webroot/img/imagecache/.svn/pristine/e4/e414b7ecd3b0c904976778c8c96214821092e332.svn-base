#wrapper_to_container_elements {	min-height : 705px !important;}#flashMessage.message {	/*display: none*/}#wrapper_to_container_elements {	padding-top: 0 !important;}#registration_form_container {	margin: 0 20px;	width: 700px}#link_header a.highlight, #link_header ul {    position: absolute;    top: 13px;}a.highlight {    color: #225478;    font-family: Arial,Verdana;    font-size: 14px;    font-weight: bold;    text-decoration: none;}.registration_form_wrapper {	background: transparent url(../img/features-text-bg.png) repeat-y 100% 0;}.reg_form_header {	height : 100px;	padding-top: 20px}.reg_form_header h1 {	font-size: 20px;	font-weight: bold;	word-spacing: -2px;	margin: 3px 0;}.reg_form_heading {	color: #204A68;}.reg_form_title {		color: #6AA9D6;}.reg_form_note, .note_to_login_info {	font-size : 12px;	color: gray}.reg_form_note strong, .form_section label, .caption a {	color: #5A5A5A;}.reg_form_sections .form_section {	float: left;	width:350px;}.login_info, .personal_info_section {	height: 190px;}.form_section div.input {	height: 40px;}.note_to_login_info {	margin-bottom: 28px}.form_section label {	font-size: 12px;	width: 125px;	display: inline-block;	font-weight: bold}.form_section input[type="text"], .form_section input[type="password"], .form_section select {	border: 1px solid #CCC;	height: 20px;	width: 188px;	position: absolute}.form_section select {	height : 23px}#UserSecurityCode {	width: 70px;	display: inline}#UserCardExpirationDateMonth, #UserCardExpirationDateYear {	width: 90px;	display: inline}div.password label {	line-height: 12px;}.security_code_holder {	width: 310px;	height: 40px}.security_code_img {	float: right}.section_header {	color: #333;	margin-bottom: 25px}.has_personal_address {	width: 250px;}.is_corporate_account {	width: 275px;}.is_agreed {	padding-top: 15px;	/* width: 295px; */	clear: both;	margin: auto;	text-align: center;}.form_section .input.checkbox {	height: 40px;	line-height: 16px;}.caption {	margin: 0;	/*float: right;*/	font-size: 11px;	font-style: italic;	color: gray}div.submit input[type="submit"] {	background: transparent url(../img/reg_submit.png) no-repeat 0 0;	color: #fff;	border : none;	width: 105px;	height: 28px;	font-family: Arial;	font-size: 14px;	text-transform: uppercase;	margin: 15px 65px 5px 10px;	float: right;}div.error-message {	color: #f00;	font-size: 10px;}#registration_panel_package_info {	width: 270px;	background: transparent url(../img/features-text-bg-reverse.png) repeat-y 0 0;	float: right;	height: 775px;	text-align: center}#registration_panel_package_info .heading_about {	color : #39bfeb;	font-size: 40px;	margin-top: 10px}p.small_heading {	color: #909090;	font-size: 14px;	letter-spacing: 10px;	text-indent: 15px;}div.package_size {    color: #ABBC34;    font-size: 75px;}div.trial_period {	color : #d38686;	font-size: 50px;}div.free_trial {	color : #1f4766;	font-size: 35px;}div.descritions_paragraph {	margin-left: 33px;    text-align: left;    width: 207px;	color:#535353;	margin-bottom: 30px;}div.descritions_paragraph p {	margin: 15px 0;	line-height: 18px;}ul.features li {	color: #363636;	font-weight: bold;	text-align: left;	background: url(../img/package_features_icon.png) no-repeat 0 0;	margin-left: 33px;	padding: 0 0 10px 25px}div.change_package {	margin-top: 20px;	height: 70px;	background: url(../img/change_package_button.png) no-repeat 50% 50%;	padding-top: 15px}div.change_package a {	color: #fff;	font-size: 19px;	text-decoration: none;}div.change_package a:hover {	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3);}#filocity_registration_no_cc_form {	background: transparent url(../img/signup-free-cc2.png) no-repeat 0 0;	background:  url(../img/logo.png) no-repeat 290px 5px, 				transparent url(../img/signup-free-cc2.png) no-repeat 0 0;	width : 469px;	height: 550px;	position: absolute;	left: 50%;	margin-left: -230px;	top: 40px];	z-index: 20000;}#filocity_registration_no_cc_form.step2 {	background: transparent url(../img/signup-free-cc2.png) no-repeat -469px 0;}#filocity_registration_no_cc_form_wrapper {	background: transparent;	margin-top: 185px;}#filocity_registration_no_cc_form_wrapper.step2 {	margin-top: 50px;}#filocity_registration_no_cc_form_wrapper p.no_cc_req {	color: #bbb;	text-align: center;	margin-bottom: 10px;	font-size: 14px;}#filocity_registration_no_cc_form_wrapper div.submit input[type="submit"] {	background: transparent url(../img/get-started-blue.png) no-repeat 50% 50%;    height: 48px;    margin-right: 150px;    width: 160px;}#filocity_registration_no_cc_form_wrapper input[type="text"], #filocity_registration_no_cc_form_wrapper input[type="password"] {    background: none repeat scroll 0 0 transparent;    color: #CECECF;    font-family: Arial;    font-size: 16pt;    height: 25px;    margin: 10px 0 0 45px;    padding: 2px 10px;    width: 378px;	border-radius: 5px 5px 5px 5px;	border: 1px solid #CCCCCC;}#filocity_registration_no_cc_form_wrapper input {    margin-left: 1px;}.registration_select_container {	background: none repeat scroll 00 transparent;	color: #aeaeaf;	font-family: Arial;	font-size: 16pt;	height: 25px;	margin: 10px 0 0 45px;	padding: 2px 10px;	width: 378px;	/*border-radius: 5px;	border: 1px solid #ccc;*/}.registration_select_container select {	width: 50%;	float: right;	margin-right: 20px;	height: 24px;	font-size: 16px;}.state, .expdate input {	width: 35% !important;	float: left !important;}.zip input, .cvv input  {	width: 147px !important;	float: left !important;}.registration_select_container.country {	clear: both;	margin-top: 50px;}#trial_cc_no_bill_notice {	display: block;	margin-top: 5px; 	width: 100%;	text-align: center;	clear:both;		font-size: 8px;}.bg_for_no_cc #logo {	display: none; }/* class 'focus' is added by registration.js when child input element has focus */.cvv.focus:after {	content: "";	width: 60px;	height: 37px;	position: absolute;	margin-left: 10px;	margin-top: 5px;	float: right !important;	background: transparent url(../img/signup_creditcart_code.png) no-repeat 0 0;}
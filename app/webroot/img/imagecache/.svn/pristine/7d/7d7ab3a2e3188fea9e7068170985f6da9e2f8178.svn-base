#packageUpgradeModal {	width: 580px;	padding: 10px;}.package_header {	color : #4e4e4e;	font-size : 16px;	font-weight: bold}.package_note {	color : #a9a7a7;	font-size : 14px;	height : 20px;	text-align: right;}.packages_list .list {	border-bottom: 1px solid #4f81bc;	width: 100%;	background : #fcfcfc;	color: #365f9f;	margin-top: 10px;}table.package_listing {    border-collapse: collapse;	font-size: 14px;	font-weight: bold}table.package_listing tr td {	text-align: center;}table.package_listing tr.even {	background-color: #d3dfef}table.package_listing th, table.package_listing td {	height: 33px;}table.package_listing th {	border-top: 1px solid #4f81bc;    border-bottom: 1px solid #4f81bc;}table.package_listing tbody td button {	background: #fff;	border-radius: 3px;	border: 1px solid #4c6796;	height: 25px;	background: rgb(255,255,255); 	background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,1) 34%, rgba(247,247,239,1) 70%, rgba(233,224,217,1) 100%); 	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(34%,rgba(255,255,255,1)), color-stop(70%,rgba(247,247,239,1)), color-stop(100%,rgba(233,224,217,1))); 	background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 34%,rgba(247,247,239,1) 70%,rgba(233,224,217,1) 100%); 	background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 34%,rgba(247,247,239,1) 70%,rgba(233,224,217,1) 100%); 	background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 34%,rgba(247,247,239,1) 70%,rgba(233,224,217,1) 100%);	background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 34%,rgba(247,247,239,1) 70%,rgba(233,224,217,1) 100%); 	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e9e0d9',GradientType=0 ); 	color: #000;}table.package_listing tbody td button.disabled {	background: #F7F7EF;}table.package_listing tbody td button.active:hover {	background: rgb(233,224,217);	background: -moz-linear-gradient(top,  rgba(233,224,217,1) 1%, rgba(247,247,239,1) 40%, rgba(255,255,255,1) 68%, rgba(255,255,255,1) 100%); 	background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,rgba(233,224,217,1)), color-stop(40%,rgba(247,247,239,1)), color-stop(68%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,1))); 	background: -webkit-linear-gradient(top,  rgba(233,224,217,1) 1%,rgba(247,247,239,1) 40%,rgba(255,255,255,1) 68%,rgba(255,255,255,1) 100%); 	background: -o-linear-gradient(top,  rgba(233,224,217,1) 1%,rgba(247,247,239,1) 40%,rgba(255,255,255,1) 68%,rgba(255,255,255,1) 100%); 	background: -ms-linear-gradient(top,  rgba(233,224,217,1) 1%,rgba(247,247,239,1) 40%,rgba(255,255,255,1) 68%,rgba(255,255,255,1) 100%);	background: linear-gradient(to bottom,  rgba(233,224,217,1) 1%,rgba(247,247,239,1) 40%,rgba(255,255,255,1) 68%,rgba(255,255,255,1) 100%);	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e9e0d9', endColorstr='#ffffff',GradientType=0 ); }
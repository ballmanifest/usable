.clear{
	clear:both;
}
#calendar_sidebar{
	float:left;
	width:202px;
	margin-left:28px;
}
#calendar_sidebar .title{
	background: url("../img/calendar-sprites.png") repeat scroll -1px -86px transparent;
    float: left;
    height: 25px;
    line-height: 25px;
    width: 162px;
	padding: 0 10px 0 30px;
	margin-bottom:10px;
	color:#ffffff;
	font-weight:bold;
	text-decoration:none;
}
.calender_container .icon{
	background: url("../img/calendar-sprites.png") repeat scroll -1px -1px transparent;
	width:16px;
	height:16px;
	float:left;
	border:0 none;
}
.calender_container .icon.down-arrow{
	background-position:-134px -1px;
}
.calender_container .icon.checkbox{
	background-position:-58px -1px;
}
.calender_container .icon.checkbox-checked{
	background-position:-77px -1px;
}
.calender_container .icon.nav-prev-icon{
	background-position:-115px -1px;
}
.calender_container .icon.nav-next-icon{
	background-position:-96px -1px;
}
#calendar_sidebar .panel{
	float:left;
	width:100%;
}

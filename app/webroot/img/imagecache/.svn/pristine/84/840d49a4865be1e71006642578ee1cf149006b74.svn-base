#directory_dd_options {
	border: solid 1px #CCC;
	border-top: none;
	list-style: none;
	box-shadow: 0px 1px 5px #DDD;
	position: absolute;
	z-index: 2000;
	margin: 0;
	padding: 0;
	background: white;
	overflow: auto;
	width: 280px;
	display: none;
}
.dd_option {
	padding: 10px;
	display: block;
	border-bottom: solid 1px #DDD;
	overflow: hidden;
	text-decoration: none;
	color: #333;
	cursor: pointer;
	-webkit-transition: all 0.25s ease-in-out;
	-moz-transition: all 0.25s ease-in-out;
	-o-transition: all 0.25s ease-in-out;
	-ms-transition: all 0.25s ease-in-out;
}
.dd_option:hover {
	background: #F3F3F3;
	color: black;
}
.dd_select:hover, .dd_selected:hover, .dd_option:hover {
	text-decoration: none;
}
.dd_select, .dd_selected {
	overflow: hidden;
	display: block;
	padding: 10px;
	color: #5c595a;
	font-size: 16px;
	cursor: pointer;
}
.dd_pointer {
	width: 0;
	height: 0;
	right: 10px;
	top: 50%;
	margin-top: -3px;
}
.dd_pointer_down {
	border: solid 5px transparent;
	border-top: solid 5px black;
}
.dd_pointer_up{
	border: solid 5px transparent !important;
	border-bottom: solid 5px black !important;
	margin-top: -8px;
}
